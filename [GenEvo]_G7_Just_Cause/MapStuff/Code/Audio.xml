		Priority = "HIGH"
		Type = "WORLD PLAYER VOICE"
		SubmixSlider = "VOICE" >
		<PitchShift Low="-1" High="1" />
		<Sound>vshimoa</Sound> 
		<Sound>vshimob</Sound> 
		<Sound>vshimoc</Sound> 
		<Sound>vshimod</Sound> 
	</AudioEvent>
	
	<AudioEvent id="America_Battleship_VoiceAttack" inheritFrom="AudioEvent:CC_BaseUnitResponse"
